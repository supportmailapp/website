{
  "name": "supportmail-website",
  "description": "SupportMail Website built with SvelteKit & Paraglide-JS",
  "private": true,
  "version": "1.6.2",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview --port 6060 --host localhost",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "deploy": "vite build && wrangler deploy --minify",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.6",
    "@sveltejs/kit": "^2.50.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.10.9",
    "cross-env": "^7.0.3",
    "daisyui": "^5.5.14",
    "globals": "^16.5.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.1",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "schema-dts": "^1.1.5",
    "svelte": "^5.48.2",
    "svelte-check": "^4.3.5",
    "tailwindcss": "^4.1.18",
    "vite": "^6.4.1",
    "vite-plugin-devtools-json": "^0.2.1",
    "wrangler": "^4.60.0"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.9.1",
    "chart.js": "^4.5.1",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "dayjs": "^1.11.19",
    "discord-api-types": "^0.38.37",
    "discord.js": "^14.25.1",
    "jsonwebtoken": "^9.0.3",
    "ky": "^1.14.3",
    "mongoose": "^8.21.1",
    "node-cache": "^5.1.2",
    "tsc": "^2.0.4",
    "typescript": "^5.9.3",
    "zod": "^4.3.6",
    "zod-validation-error": "^4.0.2"
  },
  "packageManager": "pnpm@10.27.0",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
