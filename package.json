{
  "name": "supportmail-website",
  "description": "SupportMail Website built with SvelteKit & Paraglide-JS",
  "private": true,
  "version": "1.6.2",
  "type": "module",
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview --port 6060 --host localhost",
    "prepare": "svelte-kit sync || echo ''",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write .",
    "lint": "prettier --check . && eslint .",
    "deploy": "vite build && wrangler deploy",
    "cf-typegen": "wrangler types ./src/worker-configuration.d.ts"
  },
  "devDependencies": {
    "@sveltejs/adapter-cloudflare": "^7.2.1",
    "@sveltejs/kit": "^2.31.1",
    "@sveltejs/vite-plugin-svelte": "^5.1.1",
    "@tailwindcss/vite": "^4.1.12",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^24.3.0",
    "@types/nodemailer": "^7.0.0",
    "cross-env": "^7.0.3",
    "daisyui": "^5.0.50",
    "globals": "^16.3.0",
    "mdsvex": "^0.12.6",
    "prettier": "^3.6.2",
    "prettier-plugin-svelte": "^3.4.0",
    "prettier-plugin-tailwindcss": "^0.6.14",
    "schema-dts": "^1.1.5",
    "svelte": "^5.38.2",
    "svelte-check": "^4.3.1",
    "tailwindcss": "^4.1.12",
    "vite": "^6.3.5",
    "vite-plugin-devtools-json": "^0.2.1",
    "wrangler": "^4.30.0"
  },
  "dependencies": {
    "@inlang/paraglide-js": "^2.2.0",
    "chart.js": "^4.5.0",
    "chartjs-adapter-dayjs-4": "^1.0.4",
    "dayjs": "^1.11.13",
    "discord-api-types": "^0.38.20",
    "discord.js": "^14.21.0",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.17.1",
    "node-cache": "^5.1.2",
    "nodemailer": "^7.0.5",
    "supportmail-types": "github:supportmailapp/global-types",
    "tsc": "^2.0.4",
    "typescript": "^5.9.2",
    "zod": "^4.0.17",
    "zod-validation-error": "^4.0.1"
  },
  "packageManager": "pnpm@10.12.1",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@tailwindcss/oxide",
      "esbuild",
      "sharp",
      "workerd"
    ]
  }
}
