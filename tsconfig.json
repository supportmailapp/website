{
  "extends": "./.svelte-kit/tsconfig.json",
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "outDir": "build",
    "target": "ES6",
    "module": "ESNext",
    "moduleResolution": "bundler",
    "allowArbitraryExtensions": true /* Why tf is this needed? Remove it and see the root +layout.svelte */,
    "types": ["./src/worker-configuration.d.ts"]
  }
}
